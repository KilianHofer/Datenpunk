<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.VBox?>


<BorderPane xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.main.datenpunk.AddElementController">
   <bottom>
      <BorderPane BorderPane.alignment="CENTER">
         <left>
            <Button mnemonicParsing="false" onAction="#onCancel" text="Cancel" BorderPane.alignment="CENTER" />
         </left>
         <right>
            <Button mnemonicParsing="false" onAction="#onAccept" text="Accept" BorderPane.alignment="CENTER" />
         </right>
         <BorderPane.margin>
            <Insets bottom="5.0" left="5.0" right="5.0" top="10.0" />
         </BorderPane.margin>
      </BorderPane>
   </bottom>
   <center>
      <ScrollPane hbarPolicy="NEVER" BorderPane.alignment="CENTER">
         <VBox fx:id="settingContainer">
            <padding>
               <Insets bottom="10.0" left="10.0" right="15.0" top="10.0"/>
            </padding>
         </VBox>
      </ScrollPane>
   </center>
</BorderPane>
